<% if @page.images.present? %>
    <ul id='gallery' class="clearfix">
      <% @page.images.each do |image| %>
          <li>
            <%= link_to image_tag(image.thumbnail("140x105#c").url), image.thumbnail("800x600").url , {:rel=> "group", :title => "" }%>
          </li>
      <% end %>
    </ul>
<% end %>
